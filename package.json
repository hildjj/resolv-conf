{
  "name": "resolv-conf",
  "version": "1.0.1",
  "description": "Parse resolv.conf files",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint",
    "build": "pegjs -o lib/resolv.peg.js lib/resolv.pegjs",
    "precheck": "npm run lint && npm run test",
    "check": "npm pack --dry-run",
    "release": "npm version patch && git push --follow-tags && npm publish"
  },
  "keywords": [
    "resolv",
    "resolv.conf",
    "parser",
    "grammar",
    "dns",
    "resolve",
    "resolver"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/hildjj/resolv-conf.git"
  },
  "homepage": "https://github.com/hildjj/resolv-conf",
  "bugs": "https://github.com/hildjj/resolv-conf/issues",
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "pegjs": "^0.10.0",
    "pegjs-backtrace": "^0.2.0"
  },
  "engines": {
    "node": ">=14.15"
  }
}
